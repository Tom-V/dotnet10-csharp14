	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="ZipArchive\Source\file.txt" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="ZipArchive\archive.zip">
